    "url": "https://aws.amazon.com/javascript/"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^10.0.0",
    "hash-test-vectors": "^1.3.2",
    "jest": "^26.1.0",
    "typescript": "~4.1.2"
  },
  "dependencies": {
    "@aws-sdk/types": "3.10.0",
    "@aws-sdk/util-buffer-from": "3.10.0",
    "tslib": "^1.8.0"
  },
  "engines": {

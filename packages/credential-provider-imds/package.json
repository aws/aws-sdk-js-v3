{
  "name": "@aws-sdk/credential-provider-imds",
  "version": "1.0.0-rc.2",
  "description": "AWS credential provider that sources credentials from the EC2 instance metadata service and ECS container metadata service",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "scripts": {
    "prepublishOnly": "yarn build:cjs && yarn build:es",
    "pretest": "yarn build:cjs",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build": "yarn build:es && yarn build:cjs",
    "test": "jest"
  },
  "keywords": [
    "aws",
    "credentials"
  ],
  "author": {
    "name": "AWS SDK for JavaScript Team",
    "url": "https://aws.amazon.com/javascript/"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/property-provider": "1.0.0-rc.2",
    "@aws-sdk/types": "1.0.0-rc.2",
    "tslib": "^1.8.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^10.0.0",
    "jest": "^26.1.0",
    "nock": "^13.0.2",
    "typescript": "~4.0.2"
  },
  "types": "./dist/cjs/index.d.ts",
  "engines": {
    "node": ">= 10.0.0"
  },
  "homepage": "https://github.com/aws/aws-sdk-js-v3/tree/master/packages/credential-provider-imds",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-sdk-js-v3.git",
    "directory": "packages/credential-provider-imds"
  }
}

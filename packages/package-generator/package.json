{
  "name": "@aws-sdk/package-generator",
  "private": true,
  "version": "0.1.0-preview.10",
  "description": "Provides a command-line tool for generating the various types of packages that constitute the AWS SDK for JavaScript",
  "scripts": {
    "prepublishOnly": "tsc",
    "pretest": "tsc -p tsconfig.test.json",
    "test": "jest"
  },
  "engines": {
    "node": ">=8.10"
  },
  "author": {
    "name": "AWS SDK for JavaScript Team",
    "url": "https://aws.amazon.com/javascript/"
  },
  "license": "Apache-2.0",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "bin": {
    "generate": "./build/cli.js"
  },
  "dependencies": {
    "@aws-sdk/build-types": "^0.1.0-preview.6",
    "@aws-sdk/service-model": "^0.1.0-preview.8",
    "@aws-sdk/service-types-generator": "^0.1.0-preview.9",
    "@aws-sdk/types": "^0.1.0-preview.6",
    "glob": "^7.1.4",
    "prettier": "1.18.2",
    "rimraf": "^2.6.2",
    "semver": "^6.0.0",
    "tslib": "^1.8.0",
    "yargs": "^10.1.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/node": "^10.0.0",
    "@types/prettier": "1.18.2",
    "@types/rimraf": "^2.0.2",
    "@types/semver": "^6.0.0",
    "@types/yargs": "~8.0",
    "jest": "^24.7.1",
    "typescript": "~3.4.0"
  }
}

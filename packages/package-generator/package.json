{
  "name": "@aws/package-generator",
  "private": true,
  "version": "0.0.1",
  "description": "Provides a command-line tool for generating the various types of packages that constitute the AWS SDK for JavaScript",
  "scripts": {
    "prepublishOnly": "tsc",
    "pretest": "tsc -p tsconfig.test.json",
    "test": "jest"
  },
  "engines": {
    "node": ">=6"
  },
  "author": "aws-sdk-js@amazon.com",
  "license": "Apache-2.0",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "bin": {
    "generate": "./build/cli.js"
  },
  "dependencies": {
    "@aws/build-types": "^0.0.1",
    "@aws/service-model": "^0.0.1",
    "@aws/service-types-generator": "^0.0.1",
    "@aws/types": "^0.0.1",
    "semver": "^5.4.1",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "@types/node": "^7.0.12",
    "@types/semver": "^5.4.0",
    "@types/yargs": "^8.0",
    "jest": "^20.0.4",
    "typescript": "^2.3"
  }
}

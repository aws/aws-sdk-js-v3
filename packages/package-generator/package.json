{
    "name": "@aws-sdk/package-generator",
    "version": "0.1.0",
    "description": "Provides a command-line tool for generating the various types of packages that constitute the AWS SDK for JavaScript",
    "scripts": {
        "prepublishOnly": "tsc",
        "pretest": "tsc -p tsconfig.test.json",
        "test": "jest"
    },
    "engines": {
        "node": ">=6"
    },
    "author": {
        "name": "AWS SDK for JavaScript Team",
        "email": "aws-sdk-js@amazon.com",
        "url": "https://aws.amazon.com/javascript/"
    },
    "license": "Apache-2.0",
    "main": "./build/index.js",
    "types": "./build/index.d.ts",
    "bin": {
        "generate": "./build/cli.js"
    },
    "dependencies": {
        "@aws-sdk/build-types": "^0.1.0",
        "@aws-sdk/service-model": "^0.1.0",
        "@aws-sdk/service-types-generator": "^0.1.0",
        "@aws-sdk/types": "^0.1.0",
        "glob": "^7.1.2",
        "rimraf": "^2.6.2",
        "semver": "^5.4.1",
        "tslib": "^1.8.0",
        "yargs": "^8.0.2"
    },
    "devDependencies": {
        "@types/glob": "^5.0.35",
        "@types/jest": "^20.0.2",
        "@types/node": "^8.10.29",
        "@types/rimraf": "^2.0.2",
        "@types/semver": "^5.4.0",
        "@types/yargs": "^8.0",
        "jest": "^20.0.4",
        "typescript": "^3.0.0"
    }
}

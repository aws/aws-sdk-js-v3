{
  "name": "@aws-sdk/is-array-buffer",
  "version": "3.1.0",
  "description": "Provides a function for detecting if an argument is an ArrayBuffer",
  "scripts": {
    "prepublishOnly": "yarn build:cjs && yarn build:es",
    "pretest": "yarn build:cjs",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build": "yarn build:es && yarn build:cjs",
    "postbuild:cjs": "downlevel-dts dist/cjs dist/cjs/ts3.4",
    "postbuild:es": "downlevel-dts dist/es dist/es/ts3.4",
    "test": "jest"
  },
  "author": {
    "name": "AWS SDK for JavaScript Team",
    "url": "https://aws.amazon.com/javascript/"
  },
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "jest": "^26.1.0",
    "typescript": "~4.1.2"
  },
  "types": "./dist/cjs/index.d.ts",
  "dependencies": {
    "tslib": "^1.8.0"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "typesVersions": {
    "<3.9": {
      "dist/cjs/*": [
        "dist/cjs/ts3.4/*"
      ],
      "dist/es/*": [
        "dist/es/ts3.4/*"
      ]
    }
  },
  "homepage": "https://github.com/aws/aws-sdk-js-v3/tree/master/packages/is-array-buffer",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-sdk-js-v3.git",
    "directory": "packages/is-array-buffer"
  }
}

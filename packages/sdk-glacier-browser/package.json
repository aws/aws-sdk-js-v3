{
    "name": "@aws-sdk/sdk-glacier-browser",
    "description": "Browser SDK for Amazon Glacier",
    "version": "0.1.0",
    "scripts": {
        "clean": "npm run remove-definitions && npm run remove-maps && npm run remove-js",
        "build-documentation": "npm run clean && typedoc ./",
        "prepublishOnly": "tsc",
        "pretest": "tsc",
        "remove-definitions": "rimraf *.d.ts && rimraf ./commands/*.d.ts && rimraf ./model/*.d.ts rimraf ./types/*.d.ts",
        "remove-documentation": "rimraf ./docs",
        "remove-js": "rimraf *.js && rimraf ./commands/*.js && rimraf ./model/*.js && rimraf ./types/*.js",
        "remove-maps": "rimraf *.js.map && rimraf ./commands/*.js.map && rimraf ./model/*.js.map && rimraf ./types/*.js.map",
        "test": "exit 0"
    },
    "main": "./build/index.js",
    "types": "./build/index.d.ts",
    "author": {
        "name": "AWS SDK for JavaScript Team",
        "email": "aws-sdk-js@amazon.com",
        "url": "https://aws.amazon.com/javascript/"
    },
    "license": "Apache-2.0",
    "dependencies": {
        "tslib": "^1.8.0",
        "@aws-sdk/types": "^0.1.0",
        "@aws-sdk/config-resolver": "^0.1.0",
        "@aws-sdk/middleware-stack": "^0.1.0",
        "@aws-sdk/url-parser-browser": "^0.1.0",
        "@aws-sdk/util-base64-browser": "^0.1.0",
        "@aws-sdk/util-utf8-browser": "^0.1.0",
        "@aws-sdk/stream-collector-browser": "^0.1.0",
        "@aws-sdk/middleware-serializer": "^0.1.0",
        "@aws-sdk/protocol-rest": "^0.1.0",
        "@aws-sdk/json-builder": "^0.1.0",
        "@aws-sdk/json-parser": "^0.1.0",
        "@aws-sdk/json-error-unmarshaller": "^0.1.0",
        "@aws-sdk/fetch-http-handler": "^0.1.0",
        "@aws-sdk/core-handler": "^0.1.0",
        "@aws-sdk/middleware-content-length": "^0.1.0",
        "@aws-sdk/util-body-length-browser": "^0.1.0",
        "@aws-sdk/retry-middleware": "^0.1.0",
        "@aws-sdk/signing-middleware": "^0.1.0",
        "@aws-sdk/crypto-sha256-browser": "^0.1.0",
        "@aws-sdk/signature-v4": "^0.1.0",
        "@aws-sdk/middleware-input-default": "^0.1.0",
        "@aws-sdk/middleware-sdk-glacier": "^0.1.0",
        "@aws-sdk/add-glacier-checksum-headers-browser": "^0.1.0"
    },
    "devDependencies": {
        "@aws-sdk/client-documentation-generator": "^0.1.0",
        "rimraf": "^2.6.2",
        "typedoc": "^0.10.0",
        "typescript": "^2.6"
    }
}
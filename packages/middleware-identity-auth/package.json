{
  "name": "@aws-sdk/middleware-identity-auth",
  "version": "3.0.0",
  "description": "Middleware and Plugin for resolving identity and authentication.",
  "main": "./dist-cjs/index.js",
  "module": "./dist-es/index.js",
  "types": "./dist-types/index.d.ts",
  "scripts": {
    "build": "concurrently 'yarn:build:cjs' 'yarn:build:es' 'yarn:build:types'",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build:include:deps": "lerna run --scope $npm_package_name --include-dependencies build",
    "build:types": "tsc -p tsconfig.types.json",
    "build:types:downlevel": "downlevel-dts dist-types dist-types/ts3.4",
    "clean": "rimraf ./dist-* && rimraf *.tsbuildinfo",
    "test": "jest --passWithNoTests"
  },
  "keywords": [
    "identity",
    "auth"
  ],
  "author": {
    "name": "AWS SDK for JavaScript Team",
    "url": "https://aws.amazon.com/javascript/"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/aws/aws-sdk-js-v3/tree/main/packages/middleware-identity-auth",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-sdk-js-v3.git",
    "directory": "packages/middleware-identity-auth"
  },
  "dependencies": {
    "@aws-sdk/property-provider": "*",
    "@aws-sdk/protocol-http": "*",
    "@aws-sdk/signature-v4": "*",
    "@aws-sdk/token-providers": "*",
    "@aws-sdk/types": "*",
    "@aws-sdk/util-middleware": "*",
    "@aws-sdk/client-sts": "*",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@tsconfig/recommended": "1.0.1",
    "@types/node": "^14.14.31",
    "concurrently": "7.0.0",
    "downlevel-dts": "0.10.1",
    "rimraf": "3.0.2",
    "typedoc": "0.19.2",
    "typescript": "~4.6.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "typesVersions": {
    "<4.0": {
      "dist-types/*": [
        "dist-types/ts3.4/*"
      ]
    }
  },
  "files": [
    "dist-*"
  ],
  "browser": {
    "./dist-es/tokenDefaultProvider": "./dist-es/util/provider/tokenDefaultProvider.browser",
    "./dist-es/credentialDefaultProvider": "./dist-es/util/provider/credentialDefaultProvider.browser"
  },
  "react-native": {
    "./dist-es/tokenDefaultProvider": "./dist-es/util/provider/tokenDefaultProvider.browser",
    "./dist-es/credentialDefaultProvider": "./dist-es/util/provider/credentialDefaultProvider.browser"
  }
}

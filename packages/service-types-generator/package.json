{
    "name": "@aws/service-types-generator",
    "private": true,
    "version": "0.0.1",
    "description": "Code generator for the AWS SDK",
    "dependencies": {
        "@aws/build-types": "^0.0.1",
        "@aws/types": "^0.0.1",
        "tslib": "^1.8.0"
    },
    "devDependencies": {
        "@types/jest": "^20.0.2",
        "@types/node": "^9.4.4",
        "jest": "^20.0.4",
        "typescript": "^2.6",
        "rollup-plugin-typescript": "^0.8"
    },
    "scripts": {
        "build-internal-import-map": "node ./scripts/buildInternalImportMap.js",
        "build": "npm run build-internal-import-map && tsc",

{
  "name": "aws-sdk-js-v3",
  "private": true,
  "version": "0.2.0",
  "description": "AWS SDK for JavaScript from the future",
  "main": "index.js",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "npm run clear-build-cache && lerna clean",
    "clear-build-cache": "rimraf ./packages/*/build/*",
    "copy-models": "node ./scripts/copyModels.js",
    "update-clients": "node ./scripts/rebuildClients.js",
    "pretest": "lerna run pretest",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:aws/aws-sdk-js-v3.git"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "http://aws.amazon.com"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "glob": "^7.1.2",
    "rimraf": "^2.6.2",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/jest": "^24.0.12",
    "codecov": "^3.4.0",
    "generate-changelog": "^1.7.1",
    "husky": "^3.0.0",
    "jest": "^24.7.1",
    "lerna": "3.16.0",
    "lint-staged": "^9.0.0",
    "prettier": "1.18.2",
    "typescript": "~3.4.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "packages/**/*.{ts,md,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
{
  "name": "@aws-sdk/client-auto-scaling-node",
  "description": "Node SDK for Auto Scaling",
  "version": "0.1.0-preview.4",
  "scripts": {
    "clean": "npm run remove-definitions && npm run remove-maps && npm run remove-js",
    "build-documentation": "npm run clean && typedoc ./",
    "prepublishOnly": "tsc",
    "pretest": "tsc",
    "remove-definitions": "rimraf *.d.ts && rimraf ./commands/*.d.ts && rimraf ./model/*.d.ts rimraf ./types/*.d.ts",
    "remove-documentation": "rimraf ./docs",
    "remove-js": "rimraf *.js && rimraf ./commands/*.js && rimraf ./model/*.js && rimraf ./types/*.js",
    "remove-maps": "rimraf *.js.map && rimraf ./commands/*.js.map && rimraf ./model/*.js.map && rimraf ./types/*.js.map",
    "test": "exit 0",
    "smoke-test": "npm run pretest && node ./test/smoke/index.spec.js"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "author": {
    "name": "AWS SDK for JavaScript Team",
    "url": "https://aws.amazon.com/javascript/"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/config-resolver": "^0.1.0-preview.6",
    "@aws-sdk/core-handler": "^0.1.0-preview.6",
    "@aws-sdk/credential-provider-node": "^0.1.0-preview.8",
    "@aws-sdk/hash-node": "^0.1.0-preview.7",
    "@aws-sdk/middleware-content-length": "^0.1.0-preview.6",
    "@aws-sdk/middleware-header-default": "^0.1.0-preview.6",
    "@aws-sdk/middleware-serializer": "^0.1.0-preview.6",
    "@aws-sdk/middleware-stack": "^0.1.0-preview.7",
    "@aws-sdk/node-http-handler": "^0.1.0-preview.7",
    "@aws-sdk/protocol-query": "^0.1.0-preview.7",
    "@aws-sdk/query-builder": "^0.1.0-preview.6",
    "@aws-sdk/query-error-unmarshaller": "^0.1.0-preview.7",
    "@aws-sdk/region-provider": "^0.1.0-preview.6",
    "@aws-sdk/retry-middleware": "^0.1.0-preview.6",
    "@aws-sdk/signature-v4": "^0.1.0-preview.8",
    "@aws-sdk/signing-middleware": "^0.1.0-preview.8",
    "@aws-sdk/stream-collector-node": "^0.1.0-preview.7",
    "@aws-sdk/types": "^0.1.0-preview.6",
    "@aws-sdk/url-parser-node": "^0.1.0-preview.7",
    "@aws-sdk/util-base64-node": "^0.1.0-preview.4",
    "@aws-sdk/util-body-length-node": "^0.1.0-preview.5",
    "@aws-sdk/util-user-agent-node": "^0.1.0-preview.8",
    "@aws-sdk/util-utf8-node": "^0.1.0-preview.4",
    "@aws-sdk/xml-body-parser": "^0.1.0-preview.7",
    "tslib": "^1.8.0"
  },
  "devDependencies": {
    "@aws-sdk/client-documentation-generator": "^0.1.0-preview.3",
    "@types/jest": "^24.0.12",
    "@types/node": "^10.0.0",
    "jest": "^24.7.1",
    "rimraf": "^2.6.2",
    "typedoc": "^0.14.2",
    "typescript": "~3.4.0"
  }
}

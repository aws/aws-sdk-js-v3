{
  "name": "@aws-sdk/client-lambda-browser",
  "description": "Browser SDK for AWS Lambda",
  "version": "0.1.0-preview.2",
  "scripts": {
    "clean": "npm run remove-definitions && npm run remove-maps && npm run remove-js",
    "build-documentation": "npm run clean && typedoc ./",
    "prepublishOnly": "tsc",
    "pretest": "tsc",
    "remove-definitions": "rimraf *.d.ts && rimraf ./commands/*.d.ts && rimraf ./model/*.d.ts rimraf ./types/*.d.ts",
    "remove-documentation": "rimraf ./docs",
    "remove-js": "rimraf *.js && rimraf ./commands/*.js && rimraf ./model/*.js && rimraf ./types/*.js",
    "remove-maps": "rimraf *.js.map && rimraf ./commands/*.js.map && rimraf ./model/*.js.map && rimraf ./types/*.js.map",
    "test": "exit 0",
    "smoke-test": "karma start karma.conf"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "author": {
    "name": "AWS SDK for JavaScript Team",
    "url": "https://aws.amazon.com/javascript/"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-crypto/sha256-browser": "^0.1.0-preview.1",
    "@aws-sdk/config-resolver": "^0.1.0-preview.5",
    "@aws-sdk/core-handler": "^0.1.0-preview.5",
    "@aws-sdk/fetch-http-handler": "^0.1.0-preview.5",
    "@aws-sdk/json-builder": "^0.1.0-preview.5",
    "@aws-sdk/json-error-unmarshaller": "^0.1.0-preview.6",
    "@aws-sdk/json-parser": "^0.1.0-preview.5",
    "@aws-sdk/middleware-content-length": "^0.1.0-preview.5",
    "@aws-sdk/middleware-header-default": "^0.1.0-preview.5",
    "@aws-sdk/middleware-serializer": "^0.1.0-preview.5",
    "@aws-sdk/middleware-stack": "^0.1.0-preview.6",
    "@aws-sdk/protocol-rest": "^0.1.0-preview.7",
    "@aws-sdk/retry-middleware": "^0.1.0-preview.5",
    "@aws-sdk/signature-v4": "^0.1.0-preview.6",
    "@aws-sdk/signing-middleware": "^0.1.0-preview.6",
    "@aws-sdk/stream-collector-browser": "^0.1.0-preview.5",
    "@aws-sdk/types": "^0.1.0-preview.5",
    "@aws-sdk/url-parser-browser": "^0.1.0-preview.5",
    "@aws-sdk/util-base64-browser": "^0.1.0-preview.3",
    "@aws-sdk/util-body-length-browser": "^0.1.0-preview.3",
    "@aws-sdk/util-user-agent-browser": "^0.1.0-preview.6",
    "@aws-sdk/util-utf8-browser": "^0.1.0-preview.3",
    "tslib": "^1.8.0"
  },
  "devDependencies": {
    "@aws-sdk/client-documentation-generator": "^0.1.0-preview.3",
    "@aws-sdk/karma-credential-loader": "^0.1.0-preview.6",
    "@types/jest": "^24.0.12",
    "jasmine-core": "^3.4.0",
    "jest": "^24.7.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^2.0.1",
    "karma-typescript": "^4.0.0",
    "puppeteer": "^1.0.0",
    "rimraf": "^2.6.2",
    "typedoc": "^0.14.2",
    "typescript": "~3.4.0"
  }
}
